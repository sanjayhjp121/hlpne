.pre{
	font-size:40px;
	position: absolute;
	top: 50%;
	left: 50%;
	transform: translate(-50%,-50%);
	margin: 0;
	padding: 0;
	text-transform: uppercase;
	letter-spacing: 10px;
	color:white;

}
.pre:before{
	content: attr(data-text);
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	color: #55DDB9;
	overflow: hidden;
	animation: animate 1s linear infinite;
	border-right: 4px solid #55DDB9;
	
}

@keyframes animate{

	0%{
		width: 0;
	}
	50%{
		width: 99%;
	}
	100%{
		width: 0;
	}
}

.popup{
    position:absolute; 
    left: 50%;
    transform: translateX(-50%);
    z-index: 10; 
    background-color: #fff;
    width: 70%; 
    border-radius:8px ; 
    border: 2px solid black;
    display: none;
    padding-top: 24px;
}
.cross{
    position: absolute;
    top: 15px;
    right: 20px;
    font-size: 18px;
    cursor: pointer;
}


